<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<DialogPane fx:id="customDialog" prefHeight="91.0" prefWidth="370.0" stylesheets="@../Styles/Login.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.beetechdesktopapp.Controllers.AddAssetPopupController">
   <content>
      <AnchorPane prefWidth="358.0">
         <children>
            <Text layoutX="109.0" layoutY="78.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="input_field" text="Do you want add asset?" />
            <Button defaultButton="true" layoutX="20.0" layoutY="99.0" mnemonicParsing="false" onAction="#handleOKButtonClick" prefWidth="100.0" styleClass="login_form_outter_container" text="OK">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button layoutX="250.0" layoutY="99.0" mnemonicParsing="false" onAction="#handleCancelButtonClick" prefWidth="100.0" styleClass="login_form_outter_container" text="Cancel">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="29.0" layoutY="14.0" styleClass="title_welcome" text="CONFIRMATION REQUIRED" />
         </children>
      </AnchorPane>
   </content>
</DialogPane>
